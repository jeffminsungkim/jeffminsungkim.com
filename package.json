{
  "name": "jeffminsungkim.com",
  "version": "1.0.0",
  "description": "Awesome Personal Website",
  "license": "MIT",
  "repository": "jeffminsungkim/jeffminsungkim.com",
  "author": "JeffMinsungKim <ziznzkak@gmail.com>",
  "homepage": "https://jeffminsungkim.com",
  "keywords": [
    "jeffminsungkim",
    "jeffminsungkim.com",
    "vue",
    "nuxt",
    "blog",
    "webpage"
  ],
  "private": true,
  "scripts": {
    "test": "npm run test:unit",
    "test:unit": "jest --maxWorkers=4 --coverage test/unit",
    "test:e2e": "jest --maxWorkers=1 test/e2e",
    "coverage": "codecov",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-brands": "^5.0.9",
    "@fortawesome/vue-fontawesome": "0.0.22",
    "ajv": "^6.4.0",
    "nuxt": "^1.4.0",
    "sass-resources-loader": "^1.3.3"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.12",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "eslint": "^6.6.0",
    "eslint-friendly-formatter": "^4.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.4.0",
    "jest": "^22.4.3",
    "jsdom": "^11.6.2",
    "node-sass": "^4.8.3",
    "sass-loader": "^6.0.7",
    "vue-jest": "^2.3.0",
    "vue-server-renderer": "^2.5.16"
  }
}
